{
  "expo": {
    "name": "Tic tac",
    "slug": "tictac",
    "version": "1.0.0",
    "scheme": "sonkey-tictac-snackland",
    "orientation": "portrait",
    "icon": "./assets/icons.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/tic_tac_toe_loader_.gif",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sonkey.tictac"
    },
    "extra": {
      "eas": {
        "projectId": "1e701b3f-c0c4-4fb6-abca-6157373b07c9"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icons.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sonkey.tictac"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-build-properties",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-2052849149225040~9415729474",
    "ios_app_id": "ca-app-pub-2052849149225040~9415729474"
  }
}
